{
    
    "manifest_version": 2,
    "name": "BookmarkFolderTimestampQuantium",
    "version": "0.9",
    
    "description": "Adds a default folder name of Local Timestamp",
    "homepage_url": "https://github.com/audiohacked/BookmarkFolderTimestampQuantium",

    "applications": {
        "gecko": {
            "id": "BookmarkFolderTimestampQuantium@hyperkineticnerd.com",
            "strict_min_version": "57.0"
        }
    },
    
    "icons": {
        "48": "icons/extension-48.png",
        "96": "icons/extension-96.png"
    },
    
    "permissions": [
        "bookmarks"
    ],
    
    "browser_action": {
        "browser_style": false
    },
    
    "background": {
        "scripts": ["background.js"]
    }
}